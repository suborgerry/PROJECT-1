<script>
    const tabs = (headerSelector, tabSelector, contentSelector, activeClass, display = 'block') => {
    const header = document.querySelector(headerSelector),
          tab = document.querySelectorAll(tabSelector),
          content = document.querySelectorAll(contentSelector);

        //скрываю контент
        function hideTabContent() {
            content.forEach(item => {
                item.style.display = 'none';
            });

            tab.forEach(item => {
                item.classList.remove(activeClass);
            });
        }

        //показываю контент. аргумент i - элемент на который нажал пользователь
        function showTabContent(i = 0) {
            content[i].style.display = display;
            //добавляю класс активности
            tab[i].classList.add(activeClass);
        }

        hideTabContent();
        showTabContent();

        //отслеживаю на какой именно таб нажал пользователь
        header.addEventListener('click', (e) => {
            const target = e.target;
            if (target &&
                (target.classList.contains(tabSelector.replace(/\./, "")) || 
                target.parentNode.classList.contains(tabSelector.replace(/\./, "")))) {
                //в какой именно по счету таб клик
                tab.forEach((item, i) => {
                    if (target == item || target.parentNode == item) {
                        hideTabContent();
                        showTabContent(i);
                    }
                });
            }
        });
    };
    tabs('.js-header', '.menu_type', '.menu__wrapper', 'active');
</script>

